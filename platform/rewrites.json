// sendfile known urls
{ "type":"path", "if":"/(league|ncaa-team|nfl-team|nba-team|mlb-team|nhl-team|soccer-team)\\-apparel\\-merchandise/.*", "then#sendfile":"/index.html", "apptimize":"false" },

